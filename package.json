{
  "name": "user-info-form",
  "version": "0.0.0",
  "description": "User info form - test resct app.",
  "keywords": [
    "test task",
    "react",
    "redux"
  ],
  "bugs": {
    "url": "https://gitlab.rambler.ru/AdTech/user-info-form/issues"
  },
  "license": "ISC",
  "author": "Sergey Yastrebov <yastrebserdg@yandex.ru>",
  "man": "./README.md",
  "repository": {
    "type": "git",
    "url": "git@gitlab.rambler.ru:AdTech/user-info-form.git"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && npm run sw",
    "test": "node scripts/test.js --env=jsdom",
    "sw": "sw-precache --root='build/' --static-file-globs='build/**/!(*map*)'"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.4",
    "compass-mixins": "^0.12.10",
    "material-ui": "^0.18.7",
    "react": "^15.6.1",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-jsonschema-form": "^0.49.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-promise": "^0.5.3",
    "redux-promise-middleware": "^4.3.0",
    "spdy": "^3.4.7"
  },
  "devDependencies": {
    "ignore-styles": "^5.0.1",
    "babel-cli": "^6.24.1",
    "autoprefixer": "7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "compass-inline-image": "https://github.com/LordWerter/compass-inline-image.git",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "fs-extra": "3.0.1",
    "gulp": "^3.9.1",
    "gulp-image": "^2.8.0",
    "gulp-rename": "^1.2.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "node-minify": "^2.1.0",
    "node-sass": "^3.10.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "pug": "^2.0.0-beta6",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "string-replace-webpack-plugin": "https://github.com/CrazySquirrel/string-replace-webpack-plugin.git",
    "style-loader": "^0.18.2",
    "sw-precache": "^5.2.0",
    "sw-precache-webpack-plugin": "0.11.3",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.2",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-notifier": "^1.5.0",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
